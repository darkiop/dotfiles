% wsl - windows subsystem for linux

# wsl shuutdown
wsl --shutdown

# wsl set default distribution
wsl --set-default <Distribution Name>

# wsl set version 2
wsl --set-version <subsystem> 2

# wsl set default version
wsl --set-default-version 2

# wsl list installed
wsl --list --verbose

# wsl list available versions
wsl --list --online

# wsl terminate, e.g. 'Debian'
wsl --terminate <name>

# update wsl
wsl --update

# show wsl status
wsl --status